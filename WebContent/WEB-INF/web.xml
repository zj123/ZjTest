<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>ZjTest</display-name>
  
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>org.zj.servlet.UserServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/user</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>UserHandleServlet</servlet-name>
    <servlet-class>org.zj.servlet.UserHandleServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>UserHandleServlet</servlet-name>
    <url-pattern>/userHandle</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>UserJsonServlet</servlet-name>
    <servlet-class>org.zj.servlet.UserJsonServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>UserJsonServlet</servlet-name>
    <url-pattern>/json</url-pattern>
  </servlet-mapping>
</web-app>